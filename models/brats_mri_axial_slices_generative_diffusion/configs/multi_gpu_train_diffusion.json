{
    "diffusion": {
        "_target_": "torch.nn.parallel.DistributedDataParallel",
        "module": "$@diffusion_def.to(@device)",
        "device_ids": [
            "@device"
        ],
        "find_unused_parameters": true
    },
    "run": [
        "@load_autoencoder",
        "$@autoencoder.eval()",
        "$print('scale factor:',@scale_factor)",
        "$@train#trainer.run()"
    ]
}
