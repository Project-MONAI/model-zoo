    ],
    "network_data_format": {
        "inputs": {
            "image": {
                "type": "image",
                "format": "magnitude",
                "num_channels": 3,
                "spatial_shape": [
                    224,
                    224
                ],
                "dtype": "float32",
                "value_range": [
                    0,
                    255
                ],
                "is_patch_data": true,
                "channel_def": {
                    "0": "image"
                }
            }
        },
        "outputs": {
            "pred": {
                "type": "probability",
                "format": "classification",
                "num_channels": 1,
                "spatial_shape": [
                    1,
                    1
                ],
                "dtype": "float32",
                "is_patch_data": true,
                "value_range": [
                    0,
                    1
                ],
                "channel_def": {
                    "0": "metastasis"
                }
            }
        }
    }
