    "label_classes": "binary labels for each patch",
    "pred_classes": "scalar probability",
    "eval_metrics": {
        "accuracy": 0.9,
        "froc": 0.72
    },
    "intended_use": "This is an example, not to be used for diagnostic purposes",
    "references": [
        ""
    ],
    "network_data_format": {
        "inputs": {
            "image": {
                "type": "image",
                "format": "magnitude",
                "num_channels": 3,
                "spatial_shape": [
                    224,
                    224
                ],
                "dtype": "float32",
                "value_range": [
                    0,
                    255
                ],
                "is_patch_data": true,
                "channel_def": {
                    "0": "R",
                    "1": "G",
                    "2": "B"
                }
            }
        },
        "outputs": {
            "pred": {
                "type": "probability",
                "format": "classification",
                "num_channels": 1,
                "spatial_shape": [
                    1,
                    1
                ],
                "dtype": "float32",
                "is_patch_data": true,
                "value_range": [
                    0,
                    1
                ],
                "channel_def": {
                    "0": "metastasis"
                }
            }
        }
    }
